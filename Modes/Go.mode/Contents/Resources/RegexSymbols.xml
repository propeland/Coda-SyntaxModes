<?xml version="1.0" encoding="UTF-8"?>
<symbols>

	<symbol id="Import Statements" image="Symbol#" indentation="0">
		<regex>[^\s]*import[\s]+</regex>
	</symbol>
	
	<symbol id="Go Funtions" image="SymbolF()" indentation="1">
		<regex>^func[\s]*[\w]+\(.*\)[\w\s_\*\.]*{</regex>
		<postprocess>
			<find>func</find>
			<replace></replace>
			<find>func</find>
			<replace></replace>
			<find>[\n\r]+</find>
			<replace> </replace>
			<find>[ \t]+</find>
			<replace> </replace>
			<find>[{}]</find>
			<replace></replace>
		</postprocess>
	</symbol>
	
	<symbol id="Go Methods" image="SymbolM" indentation="1">
		<regex>^func[\s]*\(+.*\)[\w\s_\[\]()\*\.]*{</regex>
		<postprocess>
			<find>func</find>
			<replace></replace>
			<find>[\n\r]+</find>
			<replace> </replace>
			<find>[ \t]+</find>
			<replace> </replace>
			<find>[{}]</find>
			<replace></replace>
		</postprocess>
	</symbol>

	<symbol id="Type Definitions" image="SymbolT" indentation="1">
		<regex>^type[\s\t]+[\w]*.*[^{\r\n]</regex>
		<postprocess>
			<find>type[\s\t]</find>
			<replace></replace>
			<find>[\n\r]+</find>
			<replace> </replace>
			<find>[ \t]+</find>
			<replace> </replace>
			<find>[{}]</find>
			<replace></replace>
		</postprocess>
	</symbol>
	
	
</symbols>
